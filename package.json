{
  "name": "store-manager",
  "version": "1.0.0",
  "description": "Store manager UI",
  "main": "index.js",
  "scripts": {
    "test": "npm run build && npm run drop-db && npm run create-db && nyc mocha --timeout 10000  --compilers js:babel-register test/*.js",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha -- --compilers js:babel-register",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "start": "npm run create-db  && node ./_build/index.js",
    "build": "npm run clean && babel ./ -d ./_build --ignore node_modules",
    "clean": "rm -rf _build",
    "start-dev": "npm run build && node ./_build/index.js",
    "drop-db": "node ./_build/server/migration/db dropTables",
    "create-db": "node ./_build/server/migration/db createTables"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Makwe-O/store-manager.git"
  },
  "author": "Mmakwe Onyeka Blessing",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Makwe-O/store-manager/issues"
  },
  "homepage": "https://github.com/Makwe-O/store-manager#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.5",
    "nyc": "^13.1.0",
    "request": "^2.88.0",
    "should": "^13.2.3"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-register": "^6.26.0",
    "babel-watch": "^2.0.7",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.0",
    "jsonwebtoken": "^8.3.0",
    "make-runnable": "^1.3.6",
    "pg": "^7.6.0"
  }
}
